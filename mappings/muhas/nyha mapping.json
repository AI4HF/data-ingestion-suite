{
  "id": "nyha mapping",
  "url": "https://ai4hf.eu/fhir/mappings/muhas/NYHAMapping",
  "name": "NYHA Mapping",
  "title": "The mapping of the MUHAS NYHA data into AI4HF NYHA profile.",
  "isDraft": false,
  "source": [
    {
      "alias": "nyha",
      "url": "https://ai4hf.eu/fhir/schemas/muhas/NYHA",
      "joinOn": []
    }
  ],
  "context": {},
  "variable": [],
  "mapping": [
    {
      "description": "The AI4HF NYHA profile.",
      "expression": {
        "name": "nyha",
        "language": "application/fhir-template+json",
        "value": {
          "id": "{{mpp:getHashedId('Observation', pid & visit.toString() & loinc_code & date & 'NYHA')}}",
          "meta": {
            "profile": [
              "https://datatools4heart.eu/fhir/StructureDefinition/HFR-NYHAAssessment"
            ]
          },
          "resourceType": "Observation",
          "category": [
            {
              "coding": [
                {
                  "system": "http://terminology.hl7.org/CodeSystem/observation-category",
                  "code": "exam",
                  "display": "Exam"
                }
              ]
            }
          ],
          "code": {
            "coding": [
              {
                "system": "http://loinc.org",
                "code": "88020-3",
                "display": "Functional capacity NYHA",
                "@sliceName": "loincCode"
              }
            ]
          },
          "effectiveDateTime": "{{date.utl:toFhirDateTime('yyyy-MM-dd HH:mm')}}",
          "valueCodeableConcept": "{{utl:createFhirCodeableConcept('http://loinc.org', loinc_code, display)}}",
          "subject": "{{mpp:createFhirReferenceWithHashedId('Patient', pid)}}",
          "encounter": "{{mpp:createFhirReferenceWithHashedId('Encounter', pid & visit.toString())}}",
          "status": "final"
        }
      }
    }
  ]
}