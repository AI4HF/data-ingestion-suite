{
  "id": "vital sign mapping",
  "url": "https://ai4hf.eu/fhir/mappings/muhas/VitalSignMapping",
  "name": "Vital Sign Mapping",
  "title": "The mapping of the MUHAS vital sign data into AI4HF VitalSign profile.",
  "isDraft": false,
  "source": [
    {
      "alias": "vitalSign",
      "url": "https://ai4hf.eu/fhir/schemas/muhas/VitalSign",
      "joinOn": []
    }
  ],
  "context": {},
  "variable": [],
  "mapping": [
    {
      "description": "The height of the patient.",
      "precondition": {
        "name": "heightExists",
        "language": "text/fhirpath",
        "expression": "name = 'height'"
      },
      "expression": {
        "name": "height",
        "language": "application/fhir-template+json",
        "value": {
          "id": "{{mpp:getHashedId('Observation', pid & name & date)}}",
          "meta": {
            "profile": [
              "https://datatools4heart.eu/fhir/StructureDefinition/HFR-VitalSign"
            ]
          },
          "resourceType": "Observation",
          "category": [
            {
              "coding": [
                {
                  "system": "http://terminology.hl7.org/CodeSystem/observation-category",
                  "code": "vital-signs",
                  "display": "Vital Signs"
                }
              ]
            }
          ],
          "code": {
            "coding": [
              {
                "system": "http://loinc.org",
                "code": "8302-2",
                "display": "Height",
                "@sliceName": "loincCode"
              }
            ]
          },
          "effectiveDateTime": "{{date.utl:toFhirDateTime('yyyy-MM-dd HH:mm')}}",
          "valueQuantity": {
            "system": "http://unitsofmeasure.org",
            "code": "{{unit}}",
            "value": "{{value}}",
            "unit": "{{unit}}"
          },
          "subject": "{{mpp:createFhirReferenceWithHashedId('Patient', pid)}}",
          "encounter": "{{? iif(visit.exists(),mpp:createFhirReferenceWithHashedId('Encounter', pid & visit.toString()),{})}}",
          "status": "final"
        }
      }
    },
